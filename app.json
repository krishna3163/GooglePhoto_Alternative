{
    "expo": {
        "name": "gphoton",
        "slug": "gphoton",
        "version": "1.0.0",
        "orientation": "default",
        "userInterfaceStyle": "automatic",
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.krishna3163.gphoton"
        },
        "web": {
            "output": "static",
            "bundler": "metro"
        },
        "plugins": [
            "expo-router"
        ],
        "scheme": "gphoton",
        "experiments": {
            "typedRoutes": true
        }
    }
}